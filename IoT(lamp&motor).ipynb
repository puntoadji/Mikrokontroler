   "source": [
    "import serial\n",
    "#arduinoData = serial.Serial('/dev/ttyACM1', 9600)\n",
    "arduinoData = serial.Serial('COM15', 115200)\n",
    "\n",
    "while True:\n",
    "    myData = input('Kirimkan perintah (1/0): ')  \n",
    "    if myData == \"1\":\n",
    "        arduinoData.write(b'1')\n",
    "    elif myData == \"0\":  \n",
    "        arduinoData.write(b'0')"
   ]
